<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company>Mirero</Company>
    <Authors>SeungyongShim</Authors>
    <PackageProjectUrl>https://github.com/seungyongshim/SeungyongShim.Akka.Extensions.DependencyInjection</PackageProjectUrl>
    <RepositoryUrl>https://github.com/seungyongshim/SeungyongShim.Akka.Extensions.DependencyInjection.git</RepositoryUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <SignAssembly>false</SignAssembly>
    <Version>1.3.14.20</Version>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageLicenseUrl>https://raw.githubusercontent.com/seungyongshim/SeungyongShim.Akka.Extensions.DependencyInjection/main/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="LICENSE" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="LICENSE" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Akka.DI.Extensions.DependencyInjection" Version="1.3.2" />
    <PackageReference Include="Scrutor" Version="3.3.0" />
  </ItemGroup>

</Project>
